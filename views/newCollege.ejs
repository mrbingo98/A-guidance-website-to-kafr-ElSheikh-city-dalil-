<%- include('partials/header'); -%>

    <div class="container green">
        <div class="login-box">
            <h3>Add A New College</h3>
            <form action="/colleges" method="POST" enctype="multipart/form-data">
                <div class="user-box">
                    <input type="text" class="form-control" id="validationDefault01" name="name" required>
                    <label for="validationDefault01">Name</label>
                </div>
                <div class="user-box">
                    <input type="text" class="form-control" id="validationDefault02" name="details" required>
                    <label for="validationDefault02">Detailes</label>
                </div>
                <div class="user-box">
                    <input type="text" class="form-control" id="validationDefault02" name="map" required>
                    <label for="validationDefault02">Map</label>
                </div>
                <div class="user-box">
                    <div class="custom-file">
                        <input type="file" class="custom-file-input" id="inputGroupFile01" name="image" aria-describedby="inputGroupFileAddon01" required>
                        <label class="custom-file-label" for="inputGroupFile01">Choose file</label>
                    </div>
                </div>
                <button class="submit" style="display: block;" type="submit">Submit</button>
            </form>
        </div>
    </div>
    <%- include('partials/footer'); -%>